{
  "name": "react-simplest-typeahead",
  "version": "1.1.0",
  "description": "Simplest typeahead with suggestion.",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "repository": "https://github.com/Platane/react-simplest-typeahead",
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-actions": "4.1.6",
    "@storybook/react": "4.1.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "flow-bin": "0.89.0",
    "jest": "23.6.0",
    "prettier": "1.9.2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.(spec|test).js"
    ]
  },
  "scripts": {
    "test": "NODE_ENV=test jest --verbose",
    "build": "rm -rf lib && NODE_ENV=production babel --out-dir ./lib --ignore '**/__tests__/' ./src ",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "NODE_ENV=production build-storybook"
  }
}
