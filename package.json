{
  "name": "react-simplest-typeahead",
  "version": "1.1.0",
  "description": "Simplest typeahead with suggestion.",
  "main": "lib/index.js",
  "files": ["lib/"],
  "repository": "https://github.com/Platane/react-simplest-typeahead",
  "dependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.6",
    "@storybook/react": "3.3.6",
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "flow-bin": "0.63.1",
    "jest": "22.0.4",
    "prettier": "1.9.2"
  },
  "jest": {
    "testMatch": ["**/__tests__/**/*.(spec|test).js"]
  },
  "scripts": {
    "test": "NODE_ENV=test jest --verbose",
    "build":
      "rm -rf lib && NODE_ENV=production babel --out-dir ./lib --ignore '**/__tests__/' ./src ",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "NODE_ENV=production build-storybook"
  }
}
