{
  "name": "react-simplest-typeahead",
  "version": "1.0.2",
  "description": "Simplest typeahead with suggestion.",
  "main": "lib/index.js",
  "files": ["lib/"],
  "repository": "https://github.com/Platane/react-simplest-typeahead",
  "dependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.6",
    "@storybook/react": "3.3.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "22.0.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "flow-bin": "0.63.1",
    "jest": "22.0.4",
    "prettier": "1.9.2"
  },
  "jest": {
    "testMatch": ["**/__tests__/**/*.(spec|test).js"]
  },
  "scripts": {
    "test": "NODE_ENV=test jest --verbose",
    "build":
      "rm -rf lib && NODE_ENV=production babel --out-dir ./lib --ignore '**/__tests__/' ./src ",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "NODE_ENV=production build-storybook"
  }
}
